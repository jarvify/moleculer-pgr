{
  "name": "moleculer-pgr-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "dev:docker": "docker-compose -f ./docker/docker-compose.yml build",
    "dev:watch": "ts-node src/dev.watch.ts",
    "dev:gen:pgr": "TS_NODE_LOG_ERROR=true ts-node src/dev.gen.pgr.ts",
    "dev:mig:gen": "npm run dev:mig:run && npm run typeorm -- migration:generate -n migration -f src/typeorm/config.ts -c $NAME",
    "dev:mig:create": "npm run typeorm -- migration:create -n migration -f src/typeorm/config.ts -c $NAME",
    "dev:mig:show": "npm run typeorm -- migration:show -f src/typeorm/config.ts -c $NAME",
    "dev:mig:run": "npm run typeorm -- migration:run -f src/typeorm/config.ts -c $NAME && npm run dev:gen:pgr",
    "dev:mig:revert": "npm run typeorm -- migration:revert -f src/typeorm/config.ts -c $NAME && npm run dev:gen:pgr",
    "dev:mig:drop": "npm run typeorm -- schema:drop -f src/typeorm/config.ts -c $NAME && npm run dev:gen:pgr",
    "postinstall": "ts-patch install && patch-package"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphile-contrib/pg-many-to-many": "^1.0.0-beta.12",
    "@graphile-contrib/pg-non-null": "^1.0.3",
    "@graphile-contrib/pg-simplify-inflector": "^5.0.0-beta.1",
    "@graphile/pg-pubsub": "^4.5.0",
    "@types/bluebird": "^3.5.29",
    "@types/node-fetch": "^2.5.4",
    "@types/validator": "^12.0.1",
    "apollo-link-batch-http": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-link-ws": "^1.0.19",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphile-utils": "^4.5.6",
    "graphql": "^14.5.8",
    "graphql-binding": "^2.5.2",
    "moleculer": "^0.13.11",
    "moleculer-decorators": "^1.1.1",
    "node-fetch": "^2.6.0",
    "patch-package": "^6.2.0",
    "postgraphile": "^4.5.5",
    "postgraphile-plugin-connection-filter": "^1.1.3",
    "postgraphile-plugin-nested-mutations": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.2",
    "sequelize-typescript": "^1.0.0",
    "subscriptions-transport-ws": "^0.9.16",
    "typeorm": "^0.2.22",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@types/node": "^10.17.6",
    "@types/prettier": "^1.19.0",
    "gts": "^1.1.0",
    "moleculer-ts": "^1.0.4",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "ts-node": "^8.5.2",
    "ts-patch": "^1.0.4",
    "ts-transformer-enumerate": "^0.5.1",
    "typescript": "^3.6.4",
    "typescript-transform-paths": "^1.1.13"
  },
  "localDependencies": {
    "moleculer-pgr": "../.."
  }
}
